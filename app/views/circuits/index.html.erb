<%- model_class = Circuit -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <!-- <th><%= model_class.human_attribute_name(:id) %></th> -->
      <th><%= model_class.human_attribute_name(:active) %></th>
      <th><%= model_class.human_attribute_name(:input) %></th>
      <th><%= model_class.human_attribute_name(:breaker_number) %></th>
      <th><%= model_class.human_attribute_name(:double_breaker) %></th>
      <th><%= model_class.human_attribute_name(:breaker_size) %></th>
      <th><%= model_class.human_attribute_name(:display) %></th>
      <th><%= model_class.human_attribute_name(:elec_load_type_id) %></th>
      <th><%= model_class.human_attribute_name(:panel_id) %></th>
      <th><%= model_class.human_attribute_name(:ct_sensor_type) %></th>
      <th><%= model_class.human_attribute_name(:double_ct) %></th>
      <th><%= model_class.human_attribute_name(:channel_no) %></th>
      <!-- <th><%= model_class.human_attribute_name(:created_at) %></th> -->
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @circuits.each do |circuit| %>
      <tr>
        <!--<td><%= link_to circuit.id, circuit_path(circuit) %></td> -->
        <td><%= circuit.active %></td>
        <td><%= circuit.input %></td>
        <td><%= circuit.breaker_number %></td>
        <td><%= circuit.double_breaker %></td>
        <td><%= circuit.breaker_size %></td>
        <td><%= circuit.display %></td>
        <td><%= circuit.elec_load_type_id %></td>
        <td><%= circuit.panel_id %></td>
        <td><%= circuit.ct_sensor_type %></td>
        <td><%= circuit.double_ct %></td>
        <td><%= circuit.channel_no %></td>
       <!-- <td><%=l circuit.created_at %></td> -->
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_circuit_path(circuit), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      circuit_path(circuit),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_circuit_path,
            :class => 'btn btn-primary' %>
